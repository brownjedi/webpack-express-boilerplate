{
  "name": "typescript-webpack-express-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "npm run eslint && cross-env NODE_ENV=test node_modules/.bin/nyc --all node --harmony-proxies node_modules/.bin/ava --verbose",
    "test:watch": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "eslint": "eslint .",
    "clean": "rimraf build",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir build --source-maps inline --copy-files",
    "build": "npm-run-all clean build:server",
    "dev": "cross-env NODE_ENV=development cross-env DEBUG=salamander-backend* nodemon --watch src src/index.js --exec 'npm run eslint && node -r 'babel-register''",
    "start": "cross-env NODE_ENV=production node build/index.js"
  },
  "engines": {
    "node": "7.8.0",
    "npm": "4.2.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "babel-register",
      "./src/utils/setupTestEnv.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/utils/setupTestEnv.js",
      "src/utils/testHelpers.js",
      "node_modules",
      "build"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "Express",
    "Node"
  ],
  "author": "Sai Karthik Ginni",
  "license": "MIT",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.0",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.0",
    "hpp": "^0.2.2",
    "morgan": "^1.8.2",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-root-import": "^4.1.8",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0"
  }
}
