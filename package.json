{
  "name": "webpack-express-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "test": "npm run clean:build && npm run flow && cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 ava",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run clean:coverage && nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "eslint": "eslint .",
    "flow": "flow",
    "clean:build": "rimraf build",
    "clean:coverage": "rimraf coverage",
    "build:server": "cross-env NODE_ENV=production webpack",
    "build": "npm-run-all clean:build build:server",
    "dev": "npm run clean:build && cross-env NODE_ENV=development DEBUG=webpack-express-boilerplate* webpack --watch",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "nodemon": "nodemon --watch build --exec '(npm run flow && node build/server.js) || exit 1'"
  },
  "engines": {
    "node": "7.8.0",
    "npm": "4.2.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "verbose": true,
    "require": [
      "babel-register",
      "./src/utils/setupTestEnv.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/utils/setupTestEnv.js",
      "src/utils/testHelpers.js",
      "node_modules",
      "build"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "Express",
    "Node"
  ],
  "author": "Sai Karthik Ginni",
  "license": "MIT",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.0",
    "debug": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-validator": "^4.0.0",
    "hpp": "^0.2.2",
    "morgan": "^1.8.2",
    "source-map-support": "^0.4.15",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.80.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  }
}
